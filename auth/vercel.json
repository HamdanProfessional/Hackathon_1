{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/init-db",
      "dest": "/api/init-db.ts"
    },
    {
      "src": "/api/add-profile-columns",
      "dest": "/api/add-profile-columns.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/auth.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
